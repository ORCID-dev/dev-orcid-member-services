alRef.componentInstance.assertion = assertion;\r\n        this.ngbModalRef.result.then(\r\n          result => {\r\n            this.router.navigate(['/assertion', { outlets: { popup: null } }]);\r\n            this.ngbModalRef = null;\r\n          },\r\n          reason => {\r\n            this.router.navigate(['/assertion', { outlets: { popup: null } }]);\r\n            this.ngbModalRef = null;\r\n          }\r\n        );\r\n      }, 0);\r\n    });\r\n  }\r\n\r\n  ngOnDestroy() {\r\n    this.ngbModalRef = null;\r\n  }\r\n}\r\n"]}]}